{
  "name": "n8n-nodes-telegram-stars",
  "version": "1.0.0",
  "description": "n8n custom node for Telegram Stars payments",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "telegram",
    "stars",
    "payments"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-nodes-telegram-stars",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-telegram-stars.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' package.json",
    "lintfix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TelegramStarsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TelegramStars/TelegramStars.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.19.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.19.0"
  }
}

